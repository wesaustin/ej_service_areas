write_csv(paste0("C:/Users/gaustin/OneDrive - Environmental Protection Agency (EPA)/DWDB/Data/ucmr/zipcode_areas/",names(zip_links)[i],".csv"))
head(data.zip)
assign(names(zip_links)[i], data.zip)
unlink(temp)
}
